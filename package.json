{
  "name": "webpack-auto",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "cp -r node_modules/ionicons/dist/ src/common/font/",
    "prestart": "npm run build",
    "preserve": "npm run build",
    "start": "node demo.js",
    "m": "mongod --config /usr/local/etc/mongod.conf",
